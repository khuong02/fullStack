efault();\r\n    try {\r\n      const res = await axios.post(\"/user/upload\", {\r\n        id,\r\n        species,\r\n        nameProduct,\r\n        imageProduct,\r\n        cloudinary,\r\n      });\r\n\r\n      if (!res)\r\n        return setUploadProduct({\r\n          ...updateProduct,\r\n          errProduct: \"Product does not upload!\",\r\n        });\r\n\r\n      setUploadProduct({\r\n        ...uploadProduct,\r\n        species: \"\",\r\n        nameProduct: \"\",\r\n        errProduct: \"\",\r\n        successProduct: res.data.msg,\r\n      });\r\n      setImageProduct(false);\r\n    } catch (err) {\r\n      err.response.data.msg &&\r\n        setUploadProduct({\r\n          ...uploadProduct,\r\n          errProduct: err.response.data.msg,\r\n          successProduct: \"\",\r\n        });\r\n    }\r\n  };\r\n\r\n  return (\r\n    <section>\r\n      <div className=\"profile-user\">\r\n        <div className=\"box-profile\">\r\n          <div className=\"avatar\">\r\n            <img src={avatar ? avatar : user.avatar} alt=\"\" />\r\n            {loading && <i className=\"fas fa-spinner fa-spin loading\"></i>}\r\n            <span>\r\n              <i className=\"fas fa-camera\"></i>\r\n              <input\r\n                type=\"file\"\r\n                name=\"avatar\"\r\n                id=\"file_up\"\r\n                onChange={changeAvatar}\r\n              />\r\n            </span>\r\n          </div>\r\n          <p>\r\n            <span>Name: </span>\r\n            {user.name}\r\n          </p>\r\n          <p>\r\n            <span>Email: </span>\r\n            {user.email}\r\n          </p>\r\n          {err && ErrShow(err)}\r\n          {success && SuccessShow(success)}\r\n          <form onSubmit={handleChangePassword}>\r\n            <div className=\"changePassword\">\r\n              <label>Password mới</label>\r\n              <input\r\n                type={hide ? \"password\" : \"text\"}\r\n                name=\"password\"\r\n          